<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceScreen
        android:title="@string/pref_intake_time_title"
        android:summary="@string/pref_intake_time_summary_one"
        android:key="pref_key_intake_time_title">

    <PreferenceCategory
        android:title="@string/pref_onetime_medicine_time"
        android:key="pref_key_onetime_time_title">


        <com.jtournie.cml.tasitrack.ListPreferenceShowSummary
            android:key="pref_key_intake_onetime_hour"
            android:title="@string/pref_hour"
            android:entries="@array/hourIntakeOneTime"
            android:entryValues="@array/hourIntakeOneTime"
            android:defaultValue="21"
            />

        <com.jtournie.cml.tasitrack.ListPreferenceShowSummary
            android:key="pref_key_intake_onetime_minute"
            android:title="@string/pref_minute"
            android:entries="@array/minIntake"
            android:entryValues="@array/minIntake"
            android:defaultValue="30"
            />
    </PreferenceCategory>

    </PreferenceScreen>

</PreferenceScreen>